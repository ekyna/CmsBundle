<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping
    xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Ekyna\Bundle\CmsBundle\Entity\SeoTranslation"
            table="cms_seo_translation">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
        </id>

        <field name="title" column="title" type="string" length="255" />
        <field name="description" column="description" type="string" length="255" nullable="true" />
        <field name="keywords" column="keywords" type="string" length="255" nullable="true" />

    </entity>

</doctrine-mapping>
